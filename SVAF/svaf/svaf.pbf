name:	"Point Cloud SAC-IA Demo"
version:	"v_1.22"
pause:	10
layer{
	name:	"data"
	type:	DSP_PAIR
	bottom:	"data"
	top:	"rectify"
	data_param{
		color: true
	}
}
layer{
	name:	"rectify"
	type:	RECTIFY
	top:	"adaboost"
	rectify_param{
		filename:	"C:/svaf/data/calib/stereo_rectify_180309.dat"
		#filename:	"F:/svaf/data/calib/stereo_rectify_180309.dat"
	}
}
layer{
	name:	"adaboost"
	type:	ADABOOST
	top:	"eadp"
	adaboost_param{
		detector:	"C:/svaf/data/boost/detector_workpiece_180403.dat"
		#detector:	"F:/svaf/data/boost/detector_workpiece_180403.dat"
		sync_frame:	true
		#sync_video:	true
		sync_epipolar:	true
		thresh:		40
		nms:		0.3
	}
	show: true
}
layer{
	name:	"eadp"
	type:	EADP_MATCH
	top:	"3dpoint"
	eadp_param{
		max_disp:	24
		factor:		2560
		#r1:			30
		#r2:			500
	}
	show:	true
	#logt:	true
}
layer{
	name:	"3dpoint"
	type:	TRIANG
	top:	"camera2world"
	triang_param{
		visible:	false
		savepc:		true
		toolbox_dir:	"C:/svaf/calib/"
		calibmat_dir:	"C:/svaf/data/calib/mat_161103/"
	}
	#show: true
	#save: true
}
layer{
	name:	"camera2world"
	type:	MXMUL
	top:	"pointcloud"
	mxmul_param{
		filename:	"C:/svaf/data/calib/camera2world_rectified_180309.dat"
	}
	#save: true
}
layer{
	name:	"pointcloud"
	type:	IA_EST
	top:	"pointcloud"
	sacia_param{
		pcd_filename:	"C:/svaf/data/pcd/W_180403195734775_0001.pcd"
		ia_param{
			max_iter:	500
			min_cors:	3.0
			max_cors:	1000.0
			voxel_grid:	3.0
			norm_rad:	20.0
			feat_rad:	50.0
		}
	}
	#show: true
	#save: true
}

